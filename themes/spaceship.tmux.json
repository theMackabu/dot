{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"type": "rprompt",
			"alignment": "right",
			"segments": [
				{
					"properties": {
						"always_enabled": false
					},
					"template": "\uf0e7 {{ .String }}",
					"type": "status",
					"style": "plain",
					"foreground_templates": ["{{ if .Error }}lightRed{{ end }}"]
				},
				{
					"foreground": "lightRed",
					"type": "upgrade",
					"style": "plain"
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"properties": {
						"style": "folder"
					},
					"template": "\n<b>{{ .Path }}</b> ",
					"foreground": "cyan",
					"type": "path",
					"style": "plain"
				},
				{
					"properties": {
						"branch_icon": " \ue0a0 ",
						"branch_identical_icon": "",
						"fetch_bare_info": true,
						"fetch_status": true,
						"mapped_branches": {
							"bug/*": "üêõ ",
							"feat/*": "\ud83d\ude80 "
						},
						"source": "cli"
					},
					"template": "<b><#ffffff>in</>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}</b> ",
					"foreground": "#ff94df",
					"type": "git",
					"style": "plain",
					"foreground_templates": [
						"{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
						"{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
						"{{ if gt .Ahead 0 }}#B388FF{{ end }}",
						"{{ if gt .Behind 0 }}#B388FB{{ end }}"
					]
				},
				{
					"template": "<b><#ffffff>is</> {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }}\ued75 {{.Version}}{{ end }}{{ end }}</b> ",
					"foreground": "red",
					"type": "project",
					"style": "plain"
				},
				{
					"properties": {
						"extensions": ["package-lock.json"]
					},
					"template": "<b><#ffffff>with</> \udb82\udf15 {{ .Full }}</b> ",
					"foreground": "red",
					"type": "npm",
					"style": "plain"
				},
				{
					"properties": {
						"extensions": ["yarn.lock"]
					},
					"template": "<b><#ffffff>with</> \udb80\udd1b {{ .Full }}</b> ",
					"foreground": "#9555BB",
					"type": "yarn",
					"style": "plain"
				},
				{
					"properties": {
						"extensions": ["pnpm-lock.yaml"]
					},
					"template": "<b><#ffffff>with</> \udb82\udf17 {{ .Full }}</b> ",
					"foreground": "#F69220",
					"type": "pnpm",
					"style": "plain"
				},
				{
					"template": "<b><#ffffff>via</> \u2b22 {{ .Full }}</b> ",
					"foreground": "green",
					"type": "node",
					"style": "plain"
				},
				{
					"properties": {
						"extensions": ["deno.json"]
					},
					"template": "<b><#ffffff>via</> ü¶ï {{ .Full }}</b> ",
					"foreground": "#439EB6",
					"type": "deno",
					"style": "plain"
				},
				{
					"properties": {
						"extensions": ["next.config.js", "vite.config.ts", "tailwind.config.*"]
					},
					"template": "<b><#ffffff>with</> \ue7ba {{ .Full }}</b> ",
					"foreground": "#81a1c1",
					"type": "react",
					"style": "plain"
				},
				{
					"template": "<b><#ffffff>via</> \udb84\udc62 {{.WorkspaceName}}</b> ",
					"foreground": "#7B42BC",
					"type": "terraform",
					"style": "plain"
				},
				{
					"template": "<b><#ffffff>on</> \udb82\udd67 {{ .Project }}</b> ",
					"foreground": "#FFA000",
					"type": "firebase",
					"style": "plain"
				},
				{
					"template": "<b><#ffffff>via</> üçû {{ .Full }}</b> ",
					"foreground": "#F6F1E9",
					"type": "bun",
					"style": "plain"
				},
				{
					"template": "<b><#ffffff>via</> ü¶Ä {{ .Full }}</b> ",
					"foreground": "red",
					"type": "rust",
					"style": "plain"
				},
				{
					"properties": {
						"fetch_version": false
					},
					"template": "<b><#ffffff>via</> \ue634</b> ",
					"foreground": "#CB1DEE",
					"type": "kotlin",
					"style": "plain"
				},
				{
					"template": "<b><#ffffff>via</> \ue791 {{ .Full }}</b> ",
					"foreground": "#CC342D",
					"type": "ruby",
					"style": "plain"
				},
				{
					"template": "<b><#ffffff>via</> \ue620 {{ .Full }}</b> ",
					"foreground": "#3b3beb",
					"type": "lua",
					"style": "plain"
				},
				{
					"template": "<b><#ffffff>via</> \ue235 {{ .Full }}</b> ",
					"foreground": "#3676AB",
					"type": "python",
					"style": "plain"
				},
				{
					"template": "<b><#ffffff>via</> üêπ {{ .Full }}</b> ",
					"foreground": "#79D4FD",
					"type": "go",
					"style": "plain"
				},
				{
					"properties": {
						"style": "austin",
						"threshold": 2500
					},
					"template": "<#ffffff>in</> {{ .FormattedMs }} ",
					"foreground": "lightYellow",
					"type": "executiontime",
					"style": "plain"
				}
			],
			"newline": true
		},
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"properties": {
						"charging_icon": "\uf0e7 ",
						"discharging_icon": "\u21e3",
						"not_charging_icon": "\udb80\udc83 "
					},
					"template": "{{ if le .Percentage 20 }}{{ if not .Error }}{{ .Icon }}{{ .Percentage }}% {{ end }}{{ end }}",
					"type": "battery",
					"style": "plain",
					"foreground_templates": [
						"{{if eq \"Charging\" .State.String}}{{ if le .Percentage 10 }}#ff5722{{else}}#FFFF00{{end}}{{end}}",
						"{{if eq \"Discharging\" .State.String}}#ff5722{{end}}"
					]
				},
				{
					"properties": {
						"always_enabled": true
					},
					"template": "<b>‚ûú</b>",
					"foreground": "lightGreen",
					"type": "status",
					"style": "plain",
					"foreground_templates": ["{{ if .Error }}lightRed{{ end }}"]
				}
			],
			"newline": true
		}
	],
	"version": 3,
	"final_space": true
}
